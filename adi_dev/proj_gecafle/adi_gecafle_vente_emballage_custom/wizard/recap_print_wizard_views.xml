<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_gecafle_recap_print_wizard_form" model="ir.ui.view">
        <field name="name">gecafle.recap.print.wizard.form</field>
        <field name="model">gecafle.recap.print.wizard</field>
        <field name="arch" type="xml">
            <form string="Options d'impression">
                <sheet>
                    <group>
                        <field name="recap_id" invisible="1"/>
                        <field name="sort_by_quality"/>
                        <field name="show_commission_pct"/>
                    </group>
                </sheet>
                <footer>
                    <button name="action_print_report" string="Imprimer" type="object" class="btn-primary"/>
                    <button string="Annuler" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
