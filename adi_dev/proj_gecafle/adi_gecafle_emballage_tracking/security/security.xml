<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_emballage_tracking" model="ir.module.category">
        <field name="name">Tracking Emballages</field>
        <field name="sequence">50</field>
    </record>

    <record id="group_emballage_user" model="res.groups">
        <field name="name">Utilisateur</field>
        <field name="category_id" ref="module_category_emballage_tracking"/>
    </record>

    <record id="group_emballage_manager" model="res.groups">
        <field name="name">Responsable</field>
        <field name="category_id" ref="module_category_emballage_tracking"/>
        <field name="implied_ids" eval="[(4, ref('group_emballage_user'))]"/>
    </record>
</odoo>
