<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record id="seq_gecafle_reception_recap" model="ir.sequence">
            <field name="name">Séquence des récapitulatifs de vente</field>
            <field name="code">gecafle.reception.recap</field>
            <field name="prefix">REC/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_gecafle_bon_achat" model="ir.sequence">
            <field name="name">Séquence des bons d'achat</field>
            <field name="code">gecafle.bon.achat</field>
            <field name="prefix">BA/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>